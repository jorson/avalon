<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap/v2.2.2/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap/v2.2.2/css/datepicker.css")" rel="stylesheet" type="text/css" />
    @RenderSection("Head", false)
</head>

<body>
    <div class="navbar">
      <div class="navbar-inner">
        <a class="brand" href="#">演示工程</a>
      </div>
    </div>
    @RenderBody()
    <div id="footer" style="background-color: #f5f5f5; height:60px;">
      <div class="container">
        <p class="muted credit" style="margin-top:20px;">
            Example courtesy <a href="http://martinbean.co.uk">Martin Bean</a> and <a href="http://ryanfait.com/sticky-footer/">Ryan Fait</a>.
        </p>
      </div>
    </div>

    <script src="@Url.Content("~/Content/jquery/jquery-1.7.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/jquery/jquery.validate-1.9.0.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/jquery/tmpl/jquery.tmpl-1.0.0.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/jquery/pagination/v2.2/jquery.pagination.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/ko/knockout-2.2.0.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/ko/knockout.mapping-2.1.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/bootstrap/v2.2.2/js/bootstrap.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/bootstrap/v2.2.2/js/bootstrap-datepicker.js")" type="text/javascript"></script>
    @RenderSection("Foot", false)
</body>
</html>
